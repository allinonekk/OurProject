<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="AllInOne.Legal.Entities.BackEnd"
                   namespace="AllInOne.Legal.Entities.BackEnd">

	<class name="BankruptcyDebtInfo" table="[dbo].[BankruptcyDebtInfo]" optimistic-lock="none" >
		<id name="BankruptcyDebtInfoId" column="BankruptcyDebtInfoId" type="AnsiString" >
			<generator class="assigned"/>
		</id>
		<property name="SubmissionGroupId" column="SubmissionGroupId" type="AnsiString"/>
		<property name="ItemNo" column="ItemNo" />
		<property name="BKYDebtAmount" column="BKYDebtAmount" />
		<property name="BKYDebtSGDAmount" column="BKYDebtSGDAmount" />
		<property name="LastModifiedBy" column="LastModifiedBy" />
		<property name="LastModifiedDate" column="LastModifiedDate" />
		
		<many-to-one name="MCurrency" fetch="select" cascade="none">
			<column name="BKYDebtCurrencyCode"/>
		</many-to-one>
		
	</class>
</hibernate-mapping>
