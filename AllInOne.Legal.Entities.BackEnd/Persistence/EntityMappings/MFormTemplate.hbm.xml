<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="AllInOne.Legal.Entities.BackEnd"
                   namespace="AllInOne.Legal.Entities.BackEnd">

	<class name="MFormTemplate" table="[dbo].[MFormTemplates]" optimistic-lock="none" >
		<id name="TemplateId" column="TemplateID" >
			<generator class="assigned"/>
		</id>
		<property name="Description" column="Description" />
		<property name="EFormCount" column="EFormCount" />
		<property name="IsHidden" column="IsHidden" />
		<property name="LastModifiedBy" column="LastModifiedBy" />
		<property name="LastModifiedDate" column="LastModifiedDate" />
		<property name="ObsInd" column="ObsInd" />
		<property name="TemplateName" column="TemplateName" />
		<property name="Version" column="Version" />

	</class>
</hibernate-mapping>
