<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="AllInOne.Legal.Entities.BackEnd"
                   namespace="AllInOne.Legal.Entities.BackEnd">

  <class name="PatientInfo" table="[dbo].[PatientInfo]" optimistic-lock="none" >
    <id name="PatientId" column="PatientId">
      <generator class="assigned"/>
    </id>   
    <property name="LastModifiedBy" column="LastModifiedBy" />
    <property name="LastModifiedDate" column="LastModifiedDate" />
    <property name="UserName" column="UserName" type="AnsiString"/>
    <property name="EmailId" column="EmailId" type="AnsiString"/>
    <property name="ObsInd" column="ObsInd"/>   
  </class>
</hibernate-mapping>
