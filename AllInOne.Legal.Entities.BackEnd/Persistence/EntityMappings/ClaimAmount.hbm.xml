    <property name="ClaimType" column="ClaimType" />
    <property name="Amount" column="ClaimAmount" />
    <property name="LastModifiedBy" column="LastModifiedBy" />
    <property name="LastModifiedDate" column="LastModifiedDate" />
   
  </class>
</hibernate-mapping>
