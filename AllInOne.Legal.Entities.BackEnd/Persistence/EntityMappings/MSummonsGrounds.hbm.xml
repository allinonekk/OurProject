  <sql-query name="SaveOrUpdateMSummonsGroundsProcedure">
    exec MDM_BackEnd_SaveOrUpdateMSummonsGrounds
    :ItemNo,:GroundsText,:ObsInd,:LastModifiedBy,:LastModifiedDate,:ParamID
  </sql-query>
</hibernate-mapping>
