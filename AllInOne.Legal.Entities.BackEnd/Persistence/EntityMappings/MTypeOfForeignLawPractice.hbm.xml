<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="AllInOne.Legal.Entities.BackEnd"
                   namespace="AllInOne.Legal.Entities.BackEnd">

	<class name="MTypeOfForeignLawPractice" table="[dbo].[MTypeOfForeignLawPractice]" optimistic-lock="none" >
		<id name="ForeignLPTypeCode" column="ForeignLPTypeCode" >
			<generator class="assigned"/>
		</id>
		<property name="Description" column="Description" />
		<property name="LastModifiedBy" column="LastModifiedBy" />
		<property name="LastModifiedDate" column="LastModifiedDate" />
		<property name="ObsInd" column="ObsInd" />
		<set name="ForeignLawPracticeRegs" cascade="none" inverse="true" fetch="select">
			<key>
				<column name="ForeignLPTypeCode"/>
			</key>
			<one-to-many class="ForeignLawPracticeReg"/>
		</set>
		<set name="ForeignLPSolicitorRegs" cascade="none" inverse="true" fetch="select">
			<key>
				<column name="ForeignLPTypeCode"/>
			</key>
			<one-to-many class="ForeignLPSolicitorReg"/>
		</set>

	</class>
</hibernate-mapping>
