<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="AllInOne.Legal.Entities.BackEnd"
                   namespace="AllInOne.Legal.Entities.BackEnd">

	<class name="MServeStatus" table="[dbo].[MServeStatus]" optimistic-lock="none" >
		<id name="SerStatusCode" column="SerStatusCode" >
			<generator class="assigned"/>
		</id>
		<property name="Description" column="Description" />
		<property name="LastModifiedBy" column="LastModifiedBy" />
		<property name="LastModifiedDate" column="LastModifiedDate" />
		<property name="ObsInd" column="ObsInd" />
		<set name="ServiceOfDocDocStatusHistories" cascade="none" inverse="true" fetch="select">
			<key>
				<column name="SerStatusCode"/>
			</key>
			<one-to-many class="ServiceOfDocDocStatusHistory"/>
		</set>
		<set name="ServiceOfDocInfos" cascade="none" inverse="true" fetch="select">
			<key>
				<column name="SerStatusCode"/>
			</key>
			<one-to-many class="ServiceOfDocInfo"/>
		</set>

	</class>
</hibernate-mapping>
