<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="AllInOne.Legal.Entities.BackEnd"
                   namespace="AllInOne.Legal.Entities.BackEnd">

  <class name="NatureOfAppeal" table="[dbo].[NatureOfAppeal]" optimistic-lock="none" >
    <id name="NatureOfAppealId" column="NatureOfAppealId" type="AnsiString">
      <generator class="assigned"/>
    </id> 
    <property name="SubmissionGroupId" column="SubmissionGroupId" type="AnsiString"/>
    <property name="NatureOfAppealRemarks" column="NatureOfAppealRemarks" />
    <property name="LastModifiedBy" column="LastModifiedBy" />
    <property name="LastModifiedDate" column="LastModifiedDate" />
    <many-to-one name="MNatureOfAppeal" fetch="select" cascade="none">
      <column name="NatureOfAppealCode"/>
    </many-to-one>
  </class>
</hibernate-mapping>
