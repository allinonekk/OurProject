<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="AllInOne.Legal.Entities.BackEnd"
                   namespace="AllInOne.Legal.Entities.BackEnd">

  <class name="BOTDetails" table="[dbo].[BOTDetails]" optimistic-lock="none" >
    <id name="TaxationBOTDetailId" column="TaxationBOTDetailId" type="AnsiString">
      <generator class="assigned"/>
    </id>
    <property name="ItemNo" column="ItemNo" />
    <property name="BOTRemarks" column="BOTRemarks" />
    <property name="LastModifiedBy" column="LastModifiedBy" />
    <property name="LastModifiedDate" column="LastModifiedDate" />
    <property name="SubmissionGroupId" column="SubmissionGroupID" type="AnsiString"/>
    <many-to-one name="MBasisOfTaxationType" fetch="select" cascade="none">
      <column name="BOTTypeCode"/>
    </many-to-one>

  </class>
</hibernate-mapping>
